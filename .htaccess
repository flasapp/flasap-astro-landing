PassengerEnabled off
RewriteEngine On

# No redirigir si el host es my-wallet.flasap.com.uy
RewriteCond %{HTTP_HOST} ^my-wallet\.flasap\.com.uy$ [NC]
RewriteRule ^ - [L]

# No redirigir si es una petición al backend (flasap.com.uy/backend/api)
RewriteCond %{REQUEST_URI} ^/backend/api/ [NC]
RewriteRule ^ - [L]

