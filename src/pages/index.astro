---
import { getCollection } from "astro:content";

// Layout import â€” provides basic page elements: <head>, <nav>, <footer> etc.
import BaseLayout from "../layouts/BaseLayout.astro";
import { Image } from "astro:assets";

// Component Imports
import CallToAction from "../components/CallToAction.astro";
import Grid from "../components/Grid.astro";
import Hero from "../components/Hero.astro";
import Icon from "../components/Icon.astro";
import Pill from "../components/Pill.astro";
import PortfolioPreview from "../components/PortfolioPreview.astro";

// Page section components
import ContactCTA from "../components/ContactCTA.astro";
import Skills from "../components/Skills.astro";

// Content Fetching: List four most recent work projects
const projects = (await getCollection("work"))
  .sort((a, b) => b.data.publishDate.valueOf() - a.data.publishDate.valueOf())
  .slice(0, 4);

const FRAMEWORK_FRONT_ITEMS = [
  {
    name: "Astro",
    img: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSj5J1hbK8RhvlQ6TRoXh9neTcRfu0ZVwI5jQ&s",
  },
  {
    name: "React",
    img: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRR8XVWaf3-nrjVU68WepEwQgbW85ccZhWHOQ&s",
  },
  {
    name: "Svelte",
    img: "data:image/png;base64,",
  },
  {
    name: "Vue",
    img: "data:image/jpeg;base64,",
  },
  {
    name: "Angular",
    img: "data:image/jpeg;base64,",
  },
];

const FRAMEWORK_BACK_ITEMS = [
  {
    name: "Node",
    img: "data:image/png;base64,",
  },
  {
    name: "Laravel",
    img: "data:image/png;base64,",
  },
  {
    name: "GO",
    img: "data:image/png;base64,",
  },
];

const EXTA_ITEMS = [
  {
    name: "Git",
    img: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRWyDJvj7D9znsJPrCzjP3EVYZSjd19aKr0yA&s",
  },
  {
    name: "Github",
    img: "data:image/jpeg;base64,",
  },
  {
    name: "AWS",
    img: "data:image/jpeg;base64,",
  },
  {
    name: "Docker",
    img: "data:image/jpeg;base64,",
  },
  {
    name: "Vercel",
    img: "data:image/png;base64,",
  },
  {
    name: "MongoDB",
    img: "data:image/png;base64,",
  },
  {
    name: "MySQL",
    img: "data:image/png;base64,",
  },
  {
    name: "Netlify",
    img: "data:image/png;base64,",
  },
  {
    name: "Jira",
    img: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR2SbGzxykjfWAOGn-ieanDbUeBvfG8SpJkMA&s",
  },
];

// Full Astro Component Syntax:
// https://docs.astro.build/basics/astro-components/
---

<BaseLayout>
  <div class="stack gap-20 lg:gap-48">
    <div class="wrapper stack gap-8 lg:gap-20">
      <header class="hero">
        <Hero
          title="Hello, my name is Flavio Sacco"
          tagline="I am a Creative Developer who is currently based in Montevideo, Uruugay."
          align="start"
        >
          <div class="roles">
            <Pill><Icon icon="code" size="1.33em" /> Developer</Pill>
            <Pill><Icon icon="microphone-stage" size="1.33em" /> Tech-Lead</Pill
            >
            <Pill><Icon icon="strategy" size="1.33em" /> Project Manager</Pill>
          </div>
        </Hero>
        <div class="animated-border">
          <img
            alt="Flavio Sacco smiling in a red plaid shirt and tortoise shell glasses"
            style="border-radius: 12px"
            src="/assets/flasap-image-web.jpeg"
          />
        </div>
      </header>

      <Skills />
    </div>

    <main class="wrapper stack gap-20 lg:gap-48">
      <section class="section with-background with-cta">
        <header class="section-header stack gap-2 lg:gap-4 fade-in">
          <h3>Selected Work</h3>
          <p>
            Take a look below at some of my featured work for clients from the
            past few years.
          </p>
        </header>

        <div class="gallery portfolio-container">
          <Grid variant="offset">
            {
              projects.map((project) => (
                <li class="portfolio-item">
                  <PortfolioPreview project={project} />
                </li>
              ))
            }
          </Grid>
        </div>

        <div class="cta fade-in">
          <CallToAction href="/work/">
            View All
            <Icon icon="arrow-right" size="1.2em" />
          </CallToAction>
        </div>
      </section>

      <section class="with-background bg-variant knowledge-container">
        <div class="pendulum-left">
          <header
            class="section-header stack gap-2 lg:gap-4"
            style="text-align: left"
          >
            <h3>Frontend</h3>
            <p>These are my most used tools.</p>
          </header>
          <div class="frameworks-list">
            {
              FRAMEWORK_FRONT_ITEMS.map((framework) => (
                <img
                  class="framework-img"
                  alt={framework.name}
                  src={framework.img}
                />
              ))
            }
          </div>
        </div>

        <div class="pendulum-right">
          <header
            class="section-header stack gap-2 lg:gap-4"
            style="text-align: right"
          >
            <h3>Backend</h3>
            <p>Some knowledges of backend.</p>
          </header>
          <div class="frameworks-list">
            {
              FRAMEWORK_BACK_ITEMS.map((framework) => (
                <img
                  class="framework-img"
                  alt={framework.name}
                  src={framework.img}
                />
              ))
            }
          </div>
        </div>

        <div class="extra-knowledge">
          <header class="section-header stack gap-2 lg:gap-4">
            <h3>Extra</h3>
            <p>Another knowledge tools.</p>
          </header>
          <div class="frameworks-list extra-list">
            {
              EXTA_ITEMS.map((framework) => (
                <img
                  class="framework-img"
                  alt={framework.name}
                  src={framework.img}
                />
              ))
            }
          </div>
        </div>
        <!-- 
				<div class="gallery">
					<Grid variant="small">
						{
							['Medium', 'BuzzFeed', 'The Next Web', 'awwwards.', 'TechCrunch'].map((brand) => (
								<li class="mention-card">
									<p>{brand}</p>
								</li>
							))
						}
					</Grid>
				</div>
				-->
      </section>
    </main>

    <ContactCTA />
  </div>
</BaseLayout>

<style>
  .hero {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 2rem;
  }

  .roles {
    display: none;
  }

  .hero img {
    aspect-ratio: 5 / 4;
    object-fit: cover;
    object-position: top;
    border-radius: 1.5rem;
    box-shadow: var(--shadow-md);
  }

  @media (min-width: 50em) {
    .hero {
      display: grid;
      grid-template-columns: 6fr 4fr;
      padding-inline: 2.5rem;
      gap: 3.75rem;
    }

    .roles {
      margin-top: 0.5rem;
      display: flex;
      gap: 0.5rem;
    }

    .hero img {
      aspect-ratio: 3 / 4;
      border-radius: 4.5rem;
      object-fit: cover;
    }
  }

  .section {
    display: grid;
    gap: 2rem;
  }

  .with-background {
    position: relative;
  }

  .with-background::before {
    --hero-bg: var(--bg-image-subtle-2);

    content: "";
    position: absolute;
    pointer-events: none;
    left: 50%;
    width: 100vw;
    aspect-ratio: calc(2.25 / var(--bg-scale));
    top: 0;
    transform: translateY(-75%) translateX(-50%);
    background:
      url("/assets/backgrounds/noise.png") top center/220px repeat,
      var(--hero-bg) center center / var(--bg-gradient-size) no-repeat,
      var(--gray-999);
    background-blend-mode: overlay, normal, normal, normal;
    mix-blend-mode: var(--bg-blend-mode);
    z-index: -1;
  }

  .with-background.bg-variant::before {
    --hero-bg: var(--bg-image-subtle-1);
  }

  .section-header {
    justify-self: center;
    text-align: center;
    max-width: 50ch;
    font-size: var(--text-md);
    color: var(--gray-300);
  }

  .section-header h3 {
    font-size: var(--text-2xl);
  }

  @media (min-width: 50em) {
    .section {
      grid-template-columns: repeat(4, 1fr);
      grid-template-areas: "header header header header" "gallery gallery gallery gallery";
      gap: 5rem;
    }

    .section.with-cta {
      grid-template-areas: "header header header cta" "gallery gallery gallery gallery";
    }

    .section-header {
      grid-area: header;
      font-size: var(--text-lg);
    }

    .section-header h3 {
      font-size: var(--text-4xl);
    }

    .with-cta .section-header {
      justify-self: flex-start;
      text-align: left;
    }

    .gallery {
      grid-area: gallery;
    }

    .cta {
      grid-area: cta;
    }
  }

  .mention-card {
    display: flex;
    height: 7rem;
    justify-content: center;
    align-items: center;
    text-align: center;
    border: 1px solid var(--gray-800);
    border-radius: 1.5rem;
    color: var(--gray-300);
    background: var(--gradient-subtle);
    box-shadow: var(--shadow-sm);
  }

  @media (min-width: 50em) {
    .mention-card {
      border-radius: 1.5rem;
      height: 9.5rem;
    }
  }

  .knowledge-container {
    display: flex;
    flex-direction: column;
    width: 100%;
  }
  .extra-knowledge {
    display: flex;
    flex-direction: column;
    align-items: center;
  }

  .pendulum-left {
    transform-origin: top left;
    animation: rotateOnScroll 0.5s linear;
    animation-timeline: scroll(root);
    scroll-timeline-name: rotateTimeline;
    scroll-timeline-axis: vertical;
    scroll-timeline-start: 0%;
    scroll-timeline-end: 100%;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
  }

  .pendulum-right {
    margin-top: 4rem;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    transform-origin: top right;
    animation: rotateRightOnScroll 0.5s linear;
    animation-timeline: scroll(root);
    scroll-timeline-name: rotateTimeline;
    scroll-timeline-axis: vertical;
    scroll-timeline-start: 0%;
    scroll-timeline-end: 100%;
    text-align: right;
  }

  .fade-in-from-bottom {
    animation: fade-in-from-bottom 0.5s linear;
    animation-timeline: scroll(root);
    scroll-timeline-name: fadeTimeLine;
    scroll-timeline-axis: vertical;
    scroll-timeline-start: 0%;
    scroll-timeline-end: 100%;
  }

  .fade-from-right-scroll {
    animation: fade-from-right-scroll 0.5s linear;
    animation-timeline: scroll(root);
    scroll-timeline-name: fadeTimeLine;
    scroll-timeline-axis: vertical;
    scroll-timeline-start: 0%;
    scroll-timeline-end: 100%;
  }

  .fade-in {
    animation: fade-in 0.5s linear;
    animation-timeline: scroll(root);
    scroll-timeline-name: fadeTimeLine;
    scroll-timeline-axis: vertical;
    scroll-timeline-start: 0%;
    scroll-timeline-end: 100%;
  }

  .frameworks-list {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 1rem;
    margin-top: 16px;
  }

  .framework-img {
    width: 100px;
    height: 100px;
    object-fit: cover;
    border-radius: 0.9375rem;
  }

  .extra-list {
    gap: 0rem;
    transform-style: preserve-3d;
    transform: perspective(1000px);
  }
  .extra-list img {
    transition: 0.5s;
    filter: grayscale(100%);
    border: 1px solid var(--gray-800);
  }

  .extra-list img:hover {
    filter: grayscale(0%);
    transform: translateZ(200px);
  }
  .extra-list img:hover + img {
    filter: grayscale(30%);
    transform: translateZ(150px) rotateY(20deg);
  }
  .extra-list img:hover + img + img {
    filter: grayscale(60%);
    transform: translateZ(100px) rotateY(40deg);
  }
  .extra-list img:hover + img + img + img {
    filter: grayscale(90%);
    transform: translateZ(50px) rotateY(0deg);
  }

  .extra-list img:has(+ img:hover) {
    filter: grayscale(30%);
    transform: translateZ(150px) rotateY(-20deg);
  }
  .extra-list img:has(+ img + img:hover) {
    filter: grayscale(60%);
    transform: translateZ(100px) rotateY(-40deg);
  }
  .extra-list img:has(+ img + img + img:hover) {
    filter: grayscale(90%);
    transform: translateZ(50px) rotateY(0deg);
  }

  .animated-border {
    padding: 0.05em;
    width: 300px;
    background: transparent;
    text-align: center;
    border-radius: 10px;
    position: relative;
  }
  @property --angle {
    syntax: "<angle>";
    initial-value: 0deg;
    inherits: false;
  }

  .animated-border::after,
  .animated-border::before {
    content: "";
    position: absolute;
    height: 97%;
    width: 100%;
    background-image: conic-gradient(from var(--angle), transparent, #c561f6);
    top: 50%;
    left: 50%;
    translate: -50% -50%;
    z-index: -1;
    padding: 1px;
    border-radius: 10px;
    animation: 3s spin linear infinite;
    opacity: 0.9;
  }
  .animated-border::before {
    filter: blur(1.5rem);
    opacity: 0.8;
  }

  .border-rainbow-glow {
    margin: 150px auto;
    width: 300px;
    background: #1c1f2b;
    text-align: center;
    border-radius: 10px;
    position: relative;
  }
  .border-rainbow-glow::after,
  .border-rainbow-glow::before {
    content: "";
    position: absolute;
    height: 100%;
    width: 100%;
    background-image: conic-gradient(
      #da4167,
      #ddcad9,
      #b8bedd,
      #6fd08c,
      #414361,
      #da4167
    );
    top: 50%;
    left: 50%;
    translate: -50% -50%;
    z-index: -1;
    padding: 5px;
    border-radius: 10px;
    transition: all 0.5;
  }
  .border-rainbow-glow::before {
    filter: blur(1.5rem);
    opacity: 0.8;
  }

  @keyframes spin {
    from {
      --angle: 0deg;
    }
    to {
      --angle: 360deg;
    }
  }
  @keyframes rotateOnScroll {
    0% {
      opacity: 0;
      transform: rotate(24deg);
    }
    90% {
      opacity: 1;
      transform: rotate(0deg);
    }
  }
  @keyframes fade-from-right-scroll {
    0% {
      opacity: 0;
      transform: translateX(150px);
    }
    50% {
      transform: translateX(0px);
      opacity: 1;
    }
  }
  @keyframes rotateRightOnScroll {
    0% {
      opacity: 0;
      transform: rotate(-24deg);
    }
    90% {
      opacity: 1;
      transform: rotate(0deg);
    }
  }

  @keyframes fade-in {
    0% {
      opacity: 0;
    }
    20% {
      opacity: 1;
    }
  }

  @keyframes fade-in-from-bottom {
    0% {
      opacity: 0;
      transform: translateY(150px);
    }
    50% {
      transform: translateY(0px);
      opacity: 1;
    }
  }
</style>
